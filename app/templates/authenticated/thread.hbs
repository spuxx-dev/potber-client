{{! @glint-nocheck }}
{{#unless this.isError}}
  {{page-title this.pageTitle}}

  <Nav::Routes::Thread />

  <Routes::Thread::Page
    @thread={{this.thread}}
    @page={{this.page}}
    @lastReadPost={{@model.lastReadPost}}
  />
{{else}}
  {{page-title '404'}}
  <Misc::Error::NotFound />
{{/unless}}